<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WisdomPetMedicine.MainPage">

    <Grid ColumnDefinitions="*,*"
          RowDefinitions="*,*"
          Style="{StaticResource MyGridStyle}">

        <Label FontSize="30" 
               x:Name="categories" />
        <Label FontSize="30" 
               Grid.Column="1" 
               x:Name="products" />
        <Label FontSize="30" 
               Grid.Row="1"
               Grid.Column="1"
               x:Name="clients" />
    </Grid>

</ContentPage>
