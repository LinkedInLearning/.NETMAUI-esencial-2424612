<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WisdomPetMedicine.MainPage">

    <VerticalStackLayout>
        <Button Text="Botón" />
        <DatePicker />
        <Editor Placeholder="Esto es un Editor" HeightRequest="200"  />
        <Entry Placeholder="Esto es un Entry" />
        <Label Text="Label" />
        <Picker>
            <Picker.Items>
                <x:String>Uno</x:String>
                <x:String>Dos</x:String>
                <x:String>Tres</x:String>
            </Picker.Items>
        </Picker>
        <Stepper Minimum="0" Maximum="10" Increment="1" />
        <Switch IsToggled="false" />
        <Slider Minimum="0" Maximum="1" />
        <TimePicker />
    </VerticalStackLayout>

</ContentPage>
